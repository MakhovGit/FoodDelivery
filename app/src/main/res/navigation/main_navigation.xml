<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@+id/navigation_menu">
    <fragment
        android:id="@+id/navigation_menu"
        android:name="com.example.fooddelivery.ui.menu.MenuFragment"
        android:label="@string/navigation_menu"
        tools:layout="@layout/fragment_menu" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.fooddelivery.ui.profile.ProfileFragment"
        android:label="@string/navigation_profile"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/navigation_cart"
        android:name="com.example.fooddelivery.ui.cart.CartFragment"
        android:label="@string/navigation_cart"
        tools:layout="@layout/fragment_cart" />

</navigation>